            <if test="hp5sStart != null">#{hp5sStart},</if>
            <if test="mp5sMax != null">#{mp5sMax},</if>
            <if test="mp5sGrowth != null">#{mp5sGrowth},</if>
            <if test="mp5sStart != null">#{mp5sStart},</if>
            <if test="attackSpeedMax != null">#{attackSpeedMax},</if>
            <if test="attackRange != null and attackRange != ''">#{attackRange},</if>
            <if test="roleMain != null and roleMain != ''">#{roleMain},</if>
            <if test="roleAssist != null and roleAssist != ''">#{roleAssist},</if>
            <if test="birthdate != null">#{birthdate},</if>
        </trim>
	</insert>


	<delete id="delete" parameterType="heros">
		delete from heros
        <include refid="page_where" />
	</delete>

	<delete id="deleteByPrimaryKey" parameterType="Integer">
		delete from heros
		where id = #{id}
	</delete>


	<update id="updateByPrimaryKey" parameterType="heros">
		update heros 
		<trim prefix="set" suffixOverrides=",">
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="hpMax != null">hp_max = #{hpMax},</if>
            <if test="hpGrowth != null">hp_growth = #{hpGrowth},</if>
            <if test="hpStart != null">hp_start = #{hpStart},</if>
            <if test="mpMax != null">mp_max = #{mpMax},</if>
            <if test="mpGrowth != null">mp_growth = #{mpGrowth},</if>
