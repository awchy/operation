            <if test="mp5sGrowth != null">mp_5s_growth = #{mp5sGrowth},</if>
            <if test="mp5sStart != null">mp_5s_start = #{mp5sStart},</if>
            <if test="attackSpeedMax != null">attack_speed_max = #{attackSpeedMax},</if>
            <if test="attackRange != null and attackRange != ''">attack_range = #{attackRange},</if>
            <if test="roleMain != null and roleMain != ''">role_main = #{roleMain},</if>
            <if test="roleAssist != null and roleAssist != ''">role_assist = #{roleAssist},</if>
            <if test="birthdate != null">birthdate = #{birthdate},</if>
		</trim>
		<where>id = #{id}</where>
	</update>

