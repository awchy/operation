    <select id="selectAll" resultMap="herosResultMap">
        select <include refid="table_columns" />
        from heros
    </select>

    <select id="select" resultMap="herosResultMap">
        select <include refid="table_columns" />
        from heros
        <include refid="page_where" />
	</select>

    <select id="getCount" resultType="int" >
        select count(id) from heros
        <include refid="page_where" />
    </select>


    <select id="getByPrimaryKey" resultMap="herosResultMap" parameterType="Integer" >
		select <include refid="table_columns" />
		from heros
		where id = #{id}
	</select>
	
