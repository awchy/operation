<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ziroom.zo.repository.mybatis.repository.mybatis.mapper.HerosMapper">
	<resultMap type="heros" id="herosResultMap">
		<result property="id" column="id"/>
		<result property="name" column="name"/>
		<result property="hpMax" column="hp_max"/>
		<result property="hpGrowth" column="hp_growth"/>
		<result property="hpStart" column="hp_start"/>
		<result property="mpMax" column="mp_max"/>
		<result property="mpGrowth" column="mp_growth"/>
		<result property="mpStart" column="mp_start"/>
		<result property="attackMax" column="attack_max"/>
